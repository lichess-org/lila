{
  "name": "chart",
  "private": true,
  "type": "module",
  "dependencies": {
    "chart.js": "4.4.8",
    "chartjs-adapter-dayjs-4": "^1.0.4",
    "chartjs-plugin-datalabels": "^2.2.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "dayjs": "^1.11.19",
    "lib": "workspace:*",
    "nouislider": "^15.8.1"
  },
  "build": {
    "bundle": "src/chart.*ts"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "source": "./src/index.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": {
        "source": "./src/*.ts",
        "default": "./dist/*.js"
      }
    }
  }
}
