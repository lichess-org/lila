{
  "name": "site",
  "private": true,
  "type": "module",
  "dependencies": {
    "dialog-polyfill": "0.5.6",
    "@juggle/resize-observer": "^3.4.0",
    "lib": "workspace:*",
    "bits": "workspace:*"
  },
  "build": {
    "sync": {
      "node_modules/dialog-polyfill/dist/dialog-polyfill.esm.js": "/public/npm"
    },
    "bundle": [
      "src/site.*Embed.ts",
      {
        "module": "src/site.ts",
        "inline": "src/site.inline.ts"
      }
    ]
  }
}
