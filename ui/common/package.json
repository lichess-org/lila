{
  "name": "common",
  "version": "2.0.0",
  "private": true,
  "description": "lichess.org common utils",
  "module": "common.js",
  "type": "module",
  "typings": "common",
  "exports": {
    ".": "./dist/common.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "keywords": [
    "chess",
    "lichess"
  ],
  "author": "Thibault Duplessis",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "dialog-polyfill": "0.5.6",
    "lichess-pgn-viewer": "^2.0.0",
    "snabbdom": "^3.5.1",
    "tablesort": "^5.3.0"
  },
  "scripts": {
    "compile": "tsc",
    "dev": "tsc",
    "prod": "tsc"
  },
  "lichess": {
    "copy": {
      "src": "node_modules/dialog-polyfill/dist/dialog-polyfill.esm.js",
      "dest": "../../public/npm"
    }
  }
}
