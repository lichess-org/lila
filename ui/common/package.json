{
  "name": "common",
  "version": "2.0.0",
  "private": true,
  "description": "lichess.org common utils",
  "module": "common.js",
  "type": "module",
  "typings": "common",
  "exports": {
    ".": "./dist/common.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "keywords": [
    "chess",
    "lichess"
  ],
  "author": "Thibault Duplessis",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "compile": "tsc",
    "dev": "$npm_execpath run compile",
    "prod": "$npm_execpath run compile"
  },
  "dependencies": {
    "tablesort": "^5.3",
    "chessground": "^8.3.5",
    "snabbdom": "^3.3.1",
    "lichess-pgn-viewer": "^1"
  },
  "devDependencies": {
    "@types/cash": "8.0.0",
    "@types/dom-screen-wake-lock": "1.0.0",
    "@types/lichess": "2.0.0",
    "typescript": "^4"
  }
