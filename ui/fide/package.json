{
  "name": "fide",
  "private": true,
  "type": "module",
  "dependencies": {
    "chart.js": "4.5.1",
    "dayjs": "^1.11.19",
    "chartjs-adapter-dayjs-4": "^1.0.4",
    "lib": "workspace:*",
    "bits": "workspace:*"
  },
  "build": {
    "bundle": [
      "src/fidePlayerFollow.ts",
      "src/fidePlayerForm.ts",
      "src/fideRatingChart.ts"
    ]
  },
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "import": {
        "source": "./src/*.ts",
        "default": "./dist/*.js"
      }
    }
  }
}
