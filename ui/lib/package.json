{
  "name": "lib",
  "version": "2.0.0",
  "private": true,
  "description": "lichess.org common code",
  "author": "Thibault Duplessis",
  "license": "AGPL-3.0-or-later",
  "typings": "common",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": "./src/common.ts",
    "./*": "./src/*.ts"
  },
  "dependencies": {
    "@badrap/result": "^0.2.13",
    "@lichess-org/stockfish-web": "0.0.14",
    "@lichess-org/zerofish": "^0.0.40",
    "@types/debounce-promise": "^3.1.9",
    "debounce-promise": "^3.1.2",
    "dialog-polyfill": "0.5.6",
    "idb-keyval": "^6.2.1",
    "stockfish-mv.wasm": "^0.6.1",
    "stockfish-nnue.wasm": "1.0.0-1946a675.smolnet",
    "stockfish.js": "^10.0.2",
    "stockfish.wasm": "^0.10.0",
    "tablesort": "5.5.0"
  },
  "build": {
    "sync": {
      "node_modules/stockfish*/*.{js,wasm}": "/public/npm",
      "node_modules/dialog-polyfill/dist/dialog-polyfill.esm.js": "/public/npm",
      "node_modules/@lichess-org/stockfish-web/*.{js,wasm}": "/public/npm/stockfish-web",
      "node_modules/@lichess-org/zerofish/dist/*.{js,wasm}": "/public/npm",
      "/node_modules/@lichess-org/chessground/dist/chessground.min.js": "/public/npm"
    },
    "hash": [
      "/public/font/*.woff2",
      "/public/images/**",
      "/public/javascripts/**",
      "/public/lifat/background/**/*.webp",
      "/public/npm/stockfish-web/*",
      "/public/npm/*",
      "/public/piece/**",
      "/public/sound/**/*.mp3"
    ]
  }
}
