  "version": "2.0.0",
  "private": true,
  "description": "lichess.org chess utils",
  "type": "module",
  "module": "main.js",
  "types": "main.d.ts",
  "exports": {
    ".": "./dist/main.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
