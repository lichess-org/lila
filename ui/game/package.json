{
  "name": "game",
  "version": "2.0.0",
  "private": true,
  "description": "lichess.org game",
  "type": "module",
  "module": "game.js",
  "typings": "game.d.ts",
  "exports": {
    ".": "./dist/game.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "keywords": [
    "chess",
    "lichess",
    "play",
    "replay"
  ],
  "author": "Thibault Duplessis",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "compile": "tsc --incremental",
    "dev": "$npm_execpath run compile",
    "prod": "$npm_execpath run compile"
  },
  "dependencies": {
    "chessground": "^8.2.0",
    "snabbdom": "^3.3.1"
  },
  "devDependencies": {
    "@types/lichess": "2.0.0",
    "@types/web": "^0",
    "typescript": "^4"
  }
}
