{
  "include": ["src/**/*.ts"],
  "exclude": [],
  "compilerOptions": {
    "outDir": "./",
    "declaration": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noEmitOnError": true,
    "alwaysStrict": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "target": "es5",
    "lib": ["DOM", "ES5"]
  }
}
