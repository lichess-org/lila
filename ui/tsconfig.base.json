{
  "compilerOptions": {
    "strict": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "target": "ES2018",
    "module": "esnext",
    "lib": ["ES2018"],
    "esModuleInterop": true,
    "types": ["lichess", "web"]
  }
}
