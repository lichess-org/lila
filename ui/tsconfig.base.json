{
  "compilerOptions": {
    "strict": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "target": "ES2017",
    "module": "esnext",
    "lib": ["DOM", "ES2017", "DOM.iterable"],
    "types": ["lichess"]
  }
}
